{
  "description": "Invisible's Frontend Stack using Mobx, React and SAM Pattern",
  "name": "@invisible/samx",
  "version": "1.1.2",
  "main": "src/index.js",
  "repository": "git@github.com:invisible-tech/samx.git",
  "author": "Invisible Technologies Inc. <engineering@invisible.email>",
  "license": "MIT",
  "engine": {
    "node": "^8.10.0"
  },
  "scripts": {
    "lint": "eslint --config .eslintrc.json ./src ./test",
    "lint:fix": "eslint --fix --config .eslintrc.json ./src ./test",
    "build:dev": "fpack --development ./src/index.js --output build --name samx.dev.bundle.js --postprocess 'babel --presets env'",
    "build:prod": "fpack ./src/index.js --output build --name samx.prod.bundle.js --postprocess  'babel --presets env'",
    "test": "mocha \"test/**/*.spec.js\""
  },
  "peerDependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "dependencies": {
    "@invisible/eslint-config": "^1.3.4",
    "debug": "^3.1.0",
    "lodash": "^4.17.10",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-plugin-root-import": "^6.1.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "eslint": "^5.4.0",
    "fpack": "^0.5.4",
    "grunt": "^1.0.3",
    "mocha": "^5.2.0",
    "uglify": "^0.1.5",
    "uglify-js": "^3.4.7"
  }
}
